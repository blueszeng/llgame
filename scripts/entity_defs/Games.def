<root>
	<Implements>
		<Interface>	BaseObject	</Interface>
	</Implements>

	<Properties>
	    <income>
			<Type>			FLOAT				</Type>
			<Flags>			BASE			    </Flags>
			<Default>								</Default>
		    <Persistent>		true				</Persistent>
		</income>

	</Properties>

	<BaseMethods>

        <addIncome>
			<Arg>	FLOAT	</Arg>
		</addIncome>

		<reqChargeToPlayer>
			<Arg>	UNICODE	</Arg>
			<Arg>	FLOAT	</Arg>
		</reqChargeToPlayer>

		<!-- 客户端调用该接口请求进入大厅 -->
		<reqEnterGame>
			<!-- 玩家的mailbox -->
			<Arg>	MAILBOX	</Arg>

			<!-- 大厅的ID -->
			<Arg>	INT32	</Arg>
		</reqEnterGame>

		<!-- 客户端调用该接口请求离开大厅 -->
		<reqLeaveGame>
			<!-- 玩家的mailbox -->
			<Arg>	MAILBOX	</Arg>

			<!-- 大厅的ID -->
			<Arg>	INT32	</Arg>
		</reqLeaveGame>

		<!-- 请求游戏信息 -->
		<reqGameInfo>
		<Arg>  MAILBOX   </Arg>
		</reqGameInfo>
	</BaseMethods>

	<CellMethods>
	</CellMethods>
</root>

